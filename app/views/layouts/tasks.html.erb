<!DOCTYPE html>
<html>
<head>
  <title>Lighthouse</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>

</head>
<body>

<%= render :template => 'layouts/navbar' %>
<div class="container-fluid">
      <div class="row-fluid">
       <div class="span2 ">
        <table class="table table-striped">

          <thead><th class="container">Projects</th></thead>
          <% @projects.each do |project| %>
          <tr><td><h3><%= link_to project.name , workspace_project_tasks_path(@workspace,project) %></h3></td>
          </tr>
          <% end %>
        </table>
          
       </div>
       <div class="span2"></div>
      
       <div class="span8">
          <div class="row">
             <table><th >
             
             <h3><%= link_to  edit_workspace_path(@workspace) , class: 'brand' do %>
                 <i  class="icon icon-edit"></i><% end %>
                 
                 <%= link_to "#{@workspace.name}", workspaces_path,  title: "#{@workspace.description}", rel: 'tooltip' %></th>
             </h3>
             <th>
             <h3 class="space"}><%= link_to  edit_workspace_project_path(@workspace ,@project) , class: 'brand' do %>
               <i  class="icon icon-edit"></i><% end %>
               <%= link_to "#{@project.name}", workspace_projects_path(@workspace),  title: "#{@project.description}", rel: 'tooltip' %>
             </h3></th>
             </table>
          </div><br/>
      
          <%= yield %>
      </div>
    </div>
  </div>
        
</body>
</html>
       
  
    
  
           

  